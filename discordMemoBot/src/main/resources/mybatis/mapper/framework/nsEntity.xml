<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="nsEntity">
	<resultMap id="rmEntity" type="www.disbot.jmemo.api.framework.model.Entity">
		<id property="id" column="id" />
	</resultMap>

	<resultMap id="rmTimeEntity" extends="rmEntity"
		type="www.disbot.jmemo.api.framework.model.TimeEntity">
		<result property="regDt" column="reg_dt" />
		<result property="uptDt" column="upt_dt" />
	</resultMap>
</mapper>
